<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App3.Views.Page_SubPrincipal">
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand">
            <StackLayout Margin="0,10">
                <Button x:Name="Login" Clicked="Login_clicked" Text="Iniciar Sesion" HorizontalOptions="Center" />
            </StackLayout>
            <StackLayout Margin="0,10">
                <Button x:Name="Registro" Clicked="Registro_clicked" Text="Registrarse" HorizontalOptions="Center" />
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>